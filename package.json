{
  "name": "DetoxDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "test:e2e:ios:build": "detox build --configuration ios.sim.debug ",
    "test:e2e:android:build": "detox build --configuration android.emu.debug ",
    "test:e2e:ios:build:release": "detox build --configuration ios.sim.release ",
    "test:e2e:android:build:release": "detox build --configuration android.emu.release ",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios:release": "detox test --configuration ios.sim.debug",
    "test:e2e:android:release": "detox test --configuration android.emu.debug"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/detox": "^18.1.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.18.3",
    "eslint": "^8.19.0",
    "eslint-plugin-detox": "^1.0.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
